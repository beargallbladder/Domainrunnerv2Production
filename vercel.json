{
  "rewrites": [
    {
      "source": "/brands/:brand",
      "destination": "/reports/brands/:brand.html"
    }
  ],
  "headers": [
    {
      "source": "/brands/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    }
  ]
}

